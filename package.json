{
  "name": "tractor-plugin-visual-regression",
  "version": "0.1.0-beta.2",
  "author": "Craig Spence <craigspence0@gmail.com>",
  "repository": {
    "type": "git",
    "url": "https://github.com/phenomnomnominal/tractor-plugin-visual-regression.git"
  },
  "bugs": {
    "url": "https://github.com/phenomnomnominal/tractor-plugin-visual-regression/issues",
    "email": "craigspence0@gmail.com"
  },
  "main": "dist/server/index.js",
  "scripts": {
    "prepublish": "npm run build",
    "build": "npm run lint && npm run compile && npm run bundle",
    "bundle": "browserify src/client/index.js -o dist/client/bundle.js",
    "compile": "babel src/server/ --out-dir dist/server/ --ignore *.spec.js",
    "lint": "eslint src/**/*.js; true"
  },
  "license": "MIT",
  "engines": {
    "node": ">=0.10"
  },
  "devDependencies": {
    "babel-cli": "6.14.0",
    "babel-core": "6.14.0",
    "babel-eslint": "6.1.2",
    "babel-preset-es2015": "6.14.0",
    "babel-preset-stage-0": "6.5.0",
    "babel-register": "6.14.0",
    "brfs": "1.4.3",
    "browserify": "13.1.0",
    "browserify-ngannotate": "2.0.0",
    "browserify-shim": "3.8.12",
    "eslint": "3.4.0",
    "pngjs": "3.0.0"
  },
  "browserify": {
    "transform": [
      "brfs",
      "browserify-shim",
      "browserify-ngannotate"
    ]
  },
  "browserify-shim": {
    "angular": "angular"
  },
  "dependencies": {
    "pixelmatch": "4.0.2",
    "tractor-error-handler": "0.1.0",
    "tractor-file-structure": "0.1.0-beta.0"
  }
}
