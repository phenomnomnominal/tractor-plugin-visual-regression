{
  "name": "tractor-plugin-visual-regression",
  "version": "0.1.1-beta.2",
  "author": "Craig Spence <craigspence0@gmail.com>",
  "repository": {
    "type": "git",
    "url": "https://github.com/phenomnomnominal/tractor-plugin-visual-regression.git"
  },
  "bugs": {
    "url": "https://github.com/phenomnomnominal/tractor-plugin-visual-regression/issues",
    "email": "craigspence0@gmail.com"
  },
  "main": "dist/server/index.js",
  "scripts": {
    "prepublish": "npm run build",
    "build": "rm -rf dist && npm run lint && npm run cover && npm run compile && npm run bundle",
    "bundle": "mkdirp ./dist/client/ && touch ./dist/client/bundle.js && browserify ./src/client/index.js -o ./dist/client/bundle.js",
    "compile": "babel src/server/ --out-dir dist/server/ --ignore *.spec.js",
    "cover": "istanbul cover --config .istanbulrc.json node_modules/mocha/bin/_mocha -- --opts ./.mocharc 'src/**/*.spec.js'",
    "coveralls": "cat ./reports/coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js",
    "lint": "eslint src/**/*.js; true",
    "test": "mocha --opts ./.mocharc 'src/server/**/*.spec.js'",
    "watch": "npm-watch"
  },
  "license": "MIT",
  "engines": {
    "node": ">=0.10"
  },
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-core": "^6.24.1",
    "babel-eslint": "^7.2.3",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "babel-register": "^6.24.1",
    "brfs": "^1.4.3",
    "browserify": "^14.3.0",
    "browserify-ngannotate": "^2.0.0",
    "browserify-shim": "^3.8.14",
    "chai": "^3.5.0",
    "coveralls": "^2.13.1",
    "dirty-chai": "^1.2.2",
    "eslint": "^3.19.0",
    "istanbul": "^1.0.0-alpha.2",
    "mkdirp": "^0.5.1",
    "mocha": "^3.4.2",
    "npm-watch": "^0.1.9",
    "sinon": "^2.3.1",
    "sinon-chai": "^2.10.0"
  },
  "browserify": {
    "transform": [
      "brfs",
      "browserify-shim",
      "browserify-ngannotate"
    ]
  },
  "browserify-shim": {
    "angular": "angular"
  },
  "dependencies": {
    "bluebird": "^3.5.0",
    "lodash.debounce": "^4.0.8",
    "pixelmatch": "^4.0.2",
    "pngjs": "^3.2.0",
    "tractor-error-handler": "^0.2.1",
    "tractor-file-structure": "^0.2.1"
  }
}
